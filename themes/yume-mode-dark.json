    "tokenColors": [
        {
            "name": "Braces, operators, separators, etc.",
            "scope": [
                "meta.brace.round",
                "meta.brace.square",
                "punctuation.accessor",
                "punctuation.separator.comma",
                "punctuation.separator.key-value",
                "punctuation.definition.parameters",
                "punctuation.terminator.statement",
                "punctuation.definition.section.case-statement",
                "punctuation.section.function.begin.bracket.round",
                "punctuation.section.function.end.bracket.round",
                "punctuation.definition.typeparameters",
                "punctuation.definition.dictionary",
                "keyword.operator.arithmetic",
                "keyword.operator.logical",
                "keyword.operator.assignment",
                "keyword.operator.type.annotation",
                "keyword.operator.ternary",
                "keyword.operator.comparison",
                "keyword.operator.relational",
                "keyword.operator.custom.infix",
                "meta.import.tsx",
                "string.quoted.double.json",
                "entity.other.attribute-name",
                "storage.type.function.arrow"
            ],
            "settings": {
                "foreground": "#DBDBDD"
            }
        },
        {
            "name": "If, else, this, const, etc.",
            "scope": [
                "keyword",
                "variable.language.this",
                "variable.language.super",
                "constant.language.boolean.true",
                "constant.language.boolean.false",
                "constant.language.null",
                "constant.language.undefined",
                "support.type.property-name.json",
                "keyword.expressions-and-types.swift",
                "keyword.other.this",
                "variable.language punctuation.definition.variable.php",
                "variable.other.readwrite.instance.ruby",
                "variable.parameter.function.language.special"
            ],
            "settings": {
                "foreground": "#EF83B0"
            }
        },
        {
            "name": "Markdown",
            "scope": [
                "punctuation.definition.heading.markdown",
                "punctuation.definition.list.begin.markdown",
                "punctuation.definition.markdown",
                "entity.name.section.markdown"
            ],
            "settings": {
                "foreground": "#EF83B0"
            }
        },
        {
            "name": "Markdown markup",
            "scope": [
                "markup.bold.markdown",
                "markup.quote.markdown",
                "markup.italic.markdown",
                "markup.strikethrough.markdown",
                "markup.inline.raw.string.markdown",
                "punctuation.definition.bold.markdown",
                "punctuation.definition.italic.markdown",
                "punctuation.definition.strikethrough.markdown",
                "punctuation.definition.raw.markdown",
                "punctuation.definition.quote.begin.markdown",
                "string.other.link.title.markdown"
            ],
            "settings": {
                "foreground": "#FFEA8E"
            }
        },
        {
            "name": "Markdown links",
            "scope": ["markup.underline.link", "markup.underline.link.image"],
            "settings": {
                "foreground": "#4EB1CB"
            }
        },
        {
            "name": "User-defined class names",
            "scope": ["entity.name.type.class", "entity.name.class"],
            "settings": {
                "foreground": "#83D3F0"
            }
        },
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment", "unused.comment", "wildcard.comment"],
            "settings": {
                "foreground": "#909090"
            }
        },
        {
            "name": "JSDoc-style keywords",
            "scope": ["punctuation.definition.block.tag.jsdoc", "comment.block.documentation storage.type.class.jsdoc"],
            "settings": {
                "foreground": "#EF83B0"
            }
        },
        {
            "name": "JSDoc-style brackets",
            "scope": [
                "punctuation.definition.bracket.curly.begin.jsdoc",
                "punctuation.definition.bracket.curly.end.jsdoc"
            ],
            "settings": {
                "foreground": "#909090"
            }
        },
        {
            "name": "JSDoc-style entities",
            "scope": ["entity.name.type.instance.jsdoc"],
            "settings": {
                "foreground": "#DBDBDD"
            }
        },
        {
            "name": "Constants",
            "scope": ["constant", "variable.other.constant"],
            "settings": {
                "foreground": "#DBDBDD"
            }
        },

        {
            "name": "HTML tags",
            "scope": ["entity.name.tag"],
            "settings": {
                "foreground": "#EF83B0"
            }
        },
        {
            "name": "Function names",
            "scope": [
                "entity.name.function",
                "meta.function-call.object",
                "meta.function-call.php",
                "meta.function-call.static",
                "meta.function-call.generic",
                "meta.method-call.java meta.method",
                "meta.method.groovy",
                "support.function.builtin",
                "support.function.magic.python",
                "support.function.any-method.lua",
                "keyword.operator.function.infix"
            ],
            "settings": {
                "foreground": "#4EB1CB"
            }
        },
        {
            "name": "Function parameters",
            "scope": [
                "entity.name.variable.parameter",
                "entity.name.function.jsx",
                "meta.at-rule.function variable",
                "meta.at-rule.mixin variable",
                "meta.function.arguments variable.other.php",
                "meta.selectionset.graphql meta.arguments.graphql variable.arguments.graphql"
            ],
            "settings": {
                "foreground": "#DBDBDD"
            }
        },
        {
            "name": "Built-in magic functions and constants",
            "scope": ["support.function.magic", "support.variable", "variable.other.predefined"],
            "settings": {
                "foreground": "#DBDBDD"
            }
        },
        {
            "name": "Variable interpolation operators",
            "scope": [
                "meta.string-contents.quoted.double punctuation.definition.variable",
                "punctuation.definition.interpolation.begin",
                "punctuation.definition.interpolation.end",
                "punctuation.definition.template-expression.begin",
                "punctuation.definition.template-expression.end",
                "punctuation.section.embedded.begin",
                "punctuation.section.embedded.coffee",
                "punctuation.section.embedded.end",
                "punctuation.section.embedded.end source.php",
                "punctuation.section.embedded.end source.ruby",
                "punctuation.definition.variable.makefile"
            ],
            "settings": {
                "foreground": "#EF83B0"
            }
        },
        {
            "name": "Storage",
            "scope": [
                "meta.implementation storage.type.objc",
                "meta.interface-or-protocol storage.type.objc",
                "source.groovy storage.type.def",
                "storage.type.tsx",
                "storage.type.js.jsx",
                "storage.type.class",
                "storage.type"
            ],
            "settings": {
                "foreground": "#EF83B0"
            }
        },
        {
            "name": "Types",
            "scope": [
                "entity.name.type",
                "entity.name.scope-resolution",
                "keyword.primitive-datatypes.swift",
                "keyword.type.cs",
                "meta.protocol-list.objc",
                "meta.return-type.objc",
                "source.go storage.type",
                "source.groovy storage.type",
                "source.powershell entity.other.attribute-name",
                "storage.class.std.rust",
                "storage.type.java",
                "storage.type.primitive.java",
                "storage.type.built-in.primitive",
                "support.type.primitive"
            ],
            "settings": {
                "foreground": "#83D3F0"
            }
        },
        {
            "name": "Generics, templates and mapped type declarations",
            "scope": [
                "entity.name.type.type-parameter",
                "meta.indexer.mappedtype.declaration entity.name.type",
                "meta.type.parameters entity.name.type"
            ],
            "settings": {
                "foreground": "#83D3F0"
            }
        },
        {
            "name": "Storage modifiers",
            "scope": ["storage.modifier"],
            "settings": {
                "foreground": "#EF83B0"
            }
        },
        {
            "name": "Storage modifiers",
            "scope": ["storage.type.modifier.access.control.public.cpp"],
            "settings": {
                "foreground": "#4EB1CB"
            }
        },
        {
            "name": "Storage modifiers (import/include)",
            "scope": ["storage.modifier.import.java", "string.quoted.other.lt-gt.include"],
            "settings": {
                "foreground": "#DBDBDD"
            }
        },
        {
            "name": "RegExp string",
            "scope": [
                "string.regexp",
                "constant.other.character-class.set.regexp",
                "constant.character.escape.backslash.regexp"
            ],
            "settings": {
                "foreground": "#FFEA8E"
            }
        },
        {
            "name": "Character-class range group (regexp)",
            "scope": ["constant.other.character-class.range.regexp"],
            "settings": {
                "foreground": "#FFEA8E"
            }
        },
        {
            "name": "Character group (regexp)",
            "scope": ["punctuation.definition.character-class.regexp"],
            "settings": {
                "foreground": "#EF83B0"
            }
        },
        {
            "name": "Capture groups (regexp)",
            "scope": ["punctuation.definition.group.regexp"],
            "settings": {
                "foreground": "#EF83B0"
            }
        },
        {
            "name": "Lookaheads (regexp)",
            "scope": ["meta.assertion.look-ahead.regexp", "meta.assertion.negative-look-ahead.regexp"],
            "settings": {
                "foreground": "#EF83B0"
            }
        },
        {
            "name": "Constant escape sequences",
            "scope": ["constant.character.escape", "constant.character.string.escape", "constant.regexp"],
            "settings": {
                "foreground": "#EF83B0"
            }
        },
        {
            "name": "Strings",
            "scope": ["string", "punctiation.definition.string.begin", "beginning.punctuation.definition.string.end"],
            "settings": {
                "foreground": "#FFEA8E"
            }
        },
        {
            "name": "Numeric values",
            "scope": ["constant.numeric"],
            "settings": {
                "foreground": "#FFEA8E"
            }
        },
        {
            "name": "CSS units",
            "scope": [
                "constant.numeric.css",
                "keyword.other.unit.px.css",
                "keyword.other.unit.percentage.css",
                "keyword.other.unit.cm.css",
                "keyword.other.unit.mm.css",
                "keyword.other.unit.in.css",
                "keyword.other.unit.pt.css",
                "keyword.other.unit.pc.css",
                "keyword.other.unit.em.css",
                "keyword.other.unit.ex.css",
                "keyword.other.unit.ch.css",
                "keyword.other.unit.rem.css",
                "keyword.other.unit.lh.css",
                "keyword.other.unit.rlh.css",
                "keyword.other.unit.vw.css",
                "keyword.other.unit.vh.css",
                "keyword.other.unit.vmin.css",
                "keyword.other.unit.vmax.css",
                "keyword.other.unit.vb.css",
                "keyword.other.unit.vi.css",
                "keyword.other.unit.svw.css",
                "keyword.other.unit.svh.css",
                "keyword.other.unit.lvw.css",
                "keyword.other.unit.lvh.css",
                "keyword.other.unit.dvw.css",
                "keyword.other.unit.dvh.css",
                "keyword.other.unit.ms.css",
                "keyword.other.unit.s.css",
                "keyword.other.unit.fr.css"
            ],
            "settings": {
                "foreground": "#FFEA8E"
            }
        },
        {
            "name": "CSS selectors & attributes",
            "scope": [
                "variable.parameter.keyframe-list.css",
                "support.constant.property-value.css",
                "meta.property-value.css",
                "entity.name.tag.css",
                "entity.name.tag.wildcard.css",
                "entity.other.attribute-name.class.css",
                "entity.other.attribute-name.class.mixin.css",
                "entity.other.attribute-name.id.css",
                "entity.other.attribute-name.parent-selector.css",
                "entity.other.attribute-name.pseudo-class.css",
                "entity.other.attribute-name.pseudo-element.css",
                "support.function.misc.css"
            ],
            "settings": {
                "foreground": "#EF83B0"
            }
        }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "class": {
            "foreground": "#CAB4EC"
        },
        "class.declaration": {
            "foreground": "#83D3F0"
        },
        "interface": {
            "foreground": "#CAB4EC"
        },
        "type": {
            "foreground": "#CAB4EC"
        },
        "parameter": {
            "foreground": "#DBDBDD"
        },
        "parameter.declaration": {
            "foreground": "#DBDBDD",
            "fontStyle": "italic"
        },
        "variable": {
            "foreground": "#DBDBDD"
        },
        "variable.declaration": {
            "foreground": "#DBDBDD"
        },
        "variable.readonly": {
            "foreground": "#DBDBDD"
        },
        "property": {
            "foreground": "#DBDBDD"
        },
        "property.declaration": {
            "foreground": "#DBDBDD"
        },
        "enumMember": {
            "foreground": "#DBDBDD"
        }
    }
}
