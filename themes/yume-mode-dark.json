  "tokenColors": [
    {
      "scope": [
        "meta.brace.round",
        "meta.brace.square",
        "punctuation.accessor",
        "punctuation.definition.block",
        "punctuation.separator.comma",
        "punctuation.separator.key-value",
        "punctuation.definition.parameters",
        "punctuation.terminator.statement",
        "punctuation.definition.section.case-statement",
        "punctuation.section.function.begin.bracket.round",
        "punctuation.section.function.end.bracket.round",
        "punctuation.definition.typeparameters",
        "punctuation.definition.dictionary",
        "keyword.operator.arithmetic",
        "keyword.operator.logical",
        "keyword.operator.assignment",
        "keyword.operator.type.annotation",
        "keyword.operator.ternary",
        "keyword.operator.comparison",
        "keyword.operator.relational",
        "storage.type.function.arrow",
        "meta.import.tsx",
        "string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#DBDBDD"
      }
    },
    {
      "scope": [
        "keyword",
        "variable.language.this",
        "variable.language.super",
        "constant.language.boolean.true",
        "constant.language.boolean.false",
        "constant.language.null",
        "constant.language.undefined",
        "storage.type",
        "storage.modifier",
        "storage.modifier.async",
        "entity.name.tag",
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#EF83B0"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.type.class",
        "entity.other.inherited-class",
        "support.type.object.module",
        "support.type.primitive"
      ],
      "settings": {
        "foreground": "#CAB4EC"
      }
    },
    {
      "scope": [
        "string.quoted.single",
        "string.quoted.double",
        "string.template",
        "constant.numeric.decimal"
      ],
      "settings": {
        "foreground": "#FFEA8E"
      }
    },
    {
      "scope": [
        "punctuation.definition.comment",
        "comment.line.double-slash",
        "comment.block"
      ],
      "settings": {
        "foreground": "#79848D"
      }
    },
    {
      "scope": [
        "constant.numeric.css",
        "keyword.other.unit.px.css",
        "keyword.other.unit.percentage.css",
        "keyword.other.unit.cm.css",
        "keyword.other.unit.mm.css",
        "keyword.other.unit.in.css",
        "keyword.other.unit.pt.css",
        "keyword.other.unit.pc.css",
        "keyword.other.unit.em.css",
        "keyword.other.unit.ex.css",
        "keyword.other.unit.ch.css",
        "keyword.other.unit.rem.css",
        "keyword.other.unit.lh.css",
        "keyword.other.unit.rlh.css",
        "keyword.other.unit.vw.css",
        "keyword.other.unit.vh.css",
        "keyword.other.unit.vmin.css",
        "keyword.other.unit.vmax.css",
        "keyword.other.unit.vb.css",
        "keyword.other.unit.vi.css",
        "keyword.other.unit.svw.css",
        "keyword.other.unit.svh.css",
        "keyword.other.unit.lvw.css",
        "keyword.other.unit.lvh.css",
        "keyword.other.unit.dvw.css",
        "keyword.other.unit.dvh.css",
        "keyword.other.unit.ms.css",
        "keyword.other.unit.s.css",
        "keyword.other.unit.fr.css"
      ],
      "settings": {
        "foreground": "#FFEA8E"
      }
    },
    {
      "scope": [
        "variable.parameter.keyframe-list.css",
        "support.constant.property-value.css",
        "meta.property-value.css",
        "entity.name.tag.css",
        "entity.name.tag.wildcard.css",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.class.mixin.css",
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.parent-selector.css",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css",
        "support.function.misc.css"
      ],
      "settings": {
        "foreground": "#EF83B0"
      }
    },
    {
      "scope": [
        "support.type.property-name.css",
        "variable.css",
        "variable.argument.css",
        "meta.property-value.css",
        "punctuation.separator.list.comma.css",
        "keyword.operator.combinator.css",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#DBDBDD"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "variable.declaration": {
      "foreground": "#DBDBDD"
    },
    "variable": {
      "foreground": "#DBDBDD"
    },
    "class": {
      "foreground": "#83D3F0"
    },
    "parameter": {
      "foreground": "#DBDBDD"
    },
    "parameter.declaration": {
      "foreground": "#DBDBDD"
    },
    "property.declaration": {
      "foreground": "#DBDBDD"
    },
    "property": {
      "foreground": "#DBDBDD"
    },
    "method": {
      "foreground": "#4EB1CB"
    },
    "method.declaration": {
      "foreground": "#4EB1CB"
    },
    "method.defaultLibrary": {
      "foreground": "#4EB1CB"
    },
    "function": {
      "foreground": "#4EB1CB"
    },
    "function.declaration": {
      "foreground": "#4EB1CB"
    },
    "function.defaultLibrary": {
      "foreground": "#4EB1CB"
    }
  }
